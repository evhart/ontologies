    <!-- http://purl.org/net/curio/documents#Text -->

    <owl:Class rdf:about="#Text">
        <rdfs:subClassOf rdf:resource="&ns;Document"/>
    </owl:Class>
    


    <!-- http://purl.org/net/curio/ns#Document -->

    <owl:Class rdf:about="&ns;Document"/>
    


